<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iclsi.dao.UserDao">

    <select id="findById" parameterType="long">
    SELECT
      name
    FROM
      user
    WHERE
      user_id = #{id}
    </select>

</mapper>